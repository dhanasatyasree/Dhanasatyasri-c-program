#include<stdio.h>
int main()
{
char s[20];
printf("enter a string\n");
gets(s);
int i,count=0,|count=1,num=0;
for(i=0;s[0]!= '\0';i++)
{
if(isupper (s[i]))
{
count++;
}
else if(islower(s[i]))
{
count++;
}
else if(isdigit(s[i]))
{
num++
}
printf("count of uppercase:%d\n,count");
printf("count of lowercase:%d\n,count");
printf("count of numbers:%d\n,num");
return 0;
}
